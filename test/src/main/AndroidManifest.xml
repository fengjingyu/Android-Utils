    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />

    <application
        android:name="com.jingyu.middle.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".maintab.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".maintab.MainActivity"
            android:launchMode="singleTask">
            <!--<intent-filter>-->
            <!--<action android:name="android.intent.action.MAIN" />-->
            <!--<category android:name="android.intent.category.LAUNCHER" />-->
            <!--</intent-filter>-->
        </activity>
        <activity android:name=".TimerActivity" />
        <activity android:name=".DestroyGCActivity" />
        <activity android:name=".recyclerview.RecycleViewActivity" />
        <activity android:name=".recyclerview.WaterFallActivity" />
        <activity android:name=".PropertyAnimActivity" />
        <activity android:name=".LeakCanaryActivity" />
        <activity android:name=".HandlerActivity" />
        <activity android:name=".SystemDialogActivity" />
        <activity android:name=".ContactsActivity" />
        <activity android:name=".RequestPermissionActivity" />
        <activity android:name="com.jingyu.utilstest.CacheCleanActivity" />
        <activity android:name="com.jingyu.utilstest.LogActivity" />
        <activity android:name="com.jingyu.utilstest.SPActivity" />
