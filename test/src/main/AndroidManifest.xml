        <activity android:name="com.jingyu.utilstest.CamareActivity" />
        <activity
            android:name="com.jingyu.utilstest.TaskActivity0"
            android:launchMode="singleTask" />
        <activity android:name="com.jingyu.utilstest.TaskActivity1" />
        <activity android:name="com.jingyu.utilstest.TaskActivity2" />
        <activity android:name="com.jingyu.utilstest.TaskActivity3" />
        <activity android:name="com.jingyu.utilstest.AdapterActivity" />

        <meta-data
            android:name="MULTI_CHANNEL"
            android:value="${MULTI_CHANNEL_VALUE}" />

        <activity android:name=".PercentLayoutActivity" />
        <activity android:name=".fragmentlife.FragmentLifeActivity" />

        <receiver
            android:name=".receiver.MyReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>

        <service
            android:name=".service.MyService"
            android:enabled="true"
            android:exported="false"
            android:process=":myservice" />

    </application>

</manifest>